.community-container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.page {
  padding-bottom: 120rpx;
}
.doctor_icon{
  display: block;
  width:120rpx;
  height:120rpx;
  border: 1px solid #ccc;
  margin: 20rpx auto;
}
.doctor_card{
  text-align: center;
  font-size: 26rpx;
  padding: 20rpx;
}
/* .page__bd>.weui-panel:nth-last-child(2){
  margin-bottom: 120rpx;
} */
.doctor_name{
  font-size: 34rpx;
  color: #333;
}
.submit{
  display: block;
  width: 650rpx;
  height: 90rpx;
  background: rgba(22, 155, 213, 1);
  color: #ffffff;
  line-height: 90rpx;
  font-size: 32rpx;
  letter-spacing: 2px;
  position: fixed;
  left: 50%;
  bottom: 20rpx;
  margin-left: -325rpx;
  margin-top:130rpx;
}
.nosubmit{
  display: block;
  width: 400rpx;
  height: 80rpx;
  margin: 50rpx auto;
  background: #ccc;
  color: #ffffff;
  line-height: 80rpx;
  font-size: 32rpx;
  letter-spacing: 2px;
}
