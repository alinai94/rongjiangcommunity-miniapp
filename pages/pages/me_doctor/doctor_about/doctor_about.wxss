.doctor_icon{
  display: block;
  width:120rpx;
  height:120rpx;
  border: 1px solid #ccc;
  margin: 20rpx auto;
}
.doctor_card{
  text-align: center;
  font-size: 26rpx;
  padding: 20rpx;
}
.doctor_name{
  font-size: 34rpx;
  color: #333;
}
.submit{
  display: block;
  width: 400rpx;
  height: 80rpx;
  margin: 50rpx auto;
  background: rgba(22, 155, 213, 1);
  color: #ffffff;
  line-height: 80rpx;
  font-size: 32rpx;
  letter-spacing: 2px;
}

.telephone{
  display:inline-block;
  text-decoration:underline;
  color:rgb(23, 36, 219);
}