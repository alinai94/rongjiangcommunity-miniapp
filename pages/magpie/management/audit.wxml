<van-search placeholder="可以按姓名、手机号码、微信号搜索" v-model="value" />
<van-dialog id="van-dialog" />
<view wx:if="{{!personList||!personList.length}}">
    <view class='ant-empty'>
      <image class='ant-empty__img' src="data:image/svg+xml;base64,"/>
    </view>
    <view class="ant-empty__description">暂无数据</view>
  </view>
<view class="card"  wx:for="{{personList}}" wx:key="{{item.id}}" data-id='{{item.id}}' data-index='{{index}}'  data-opid="{{item.wechatOpenid}}"  bindtap="cardToDetail">
  <van-row>
    <van-col span="24">
      <text class="nickname">{{item.name}}</text>
      <text class="period">{{item.period}}届{{item.g3}}班</text>
      <image class="icon-sex" src="../../../images/icons/{{item.gender_self}}.png" background-size="cover"/>
    </van-col>
  </van-row>
  <van-row>
    <van-col span="18" class="card-content">微信:{{item.wechat}}</van-col>
    <van-col span="6">
      <van-button type="info" size="small" data-opid="{{item.wechatOpenid}}" hover-stop-propagation="true" catchtap="agree">同意</van-button>
    </van-col>
  </van-row>
  <van-row>
    <van-col span="24" class="card-content">手机号码:{{item.mobile}}</van-col>
  </van-row>
</view>